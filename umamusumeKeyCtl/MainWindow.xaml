<Window x:Class="umamusumeKeyCtl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:umamusumeKeyCtl"
        mc:Ignorable="d"
        Title="MainWindow" Height="{Binding WndHeight, Mode=TwoWay}" Width="{Binding WndWidth, Mode=TwoWay}" d:DataContext="{d:DesignInstance }">
    <Window.DataContext>
        <vm:MainWndVM/>
    </Window.DataContext>
    
    <StackPanel>
        <Image Source="{Binding Path=MyImage}"/>
    </StackPanel>
</Window>
